{
  "name": "puppet-bird",
  "version": "1.2.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Puppet bird Module",
  "license": "GPL-3.0",
  "source": "https://github.com/voxpupuli/puppet-bird",
  "project_page": "https://github.com/voxpupuli/puppet-bird/",
  "issues_url": "https://github.com/voxpupuli/puppet-bird/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    }
  ],
  "description": "Module to install/manage Bird internet routing daemon using puppet",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 6.0.0"
    }
  ]
}
